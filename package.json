{
  "name": "elevio",
  "license": "MIT",
  "description": "This package contains the Elevio client install packages both in React and native npm module form.",
  "keywords": [
    "Elevio",
    "elevio",
    "elev",
    "React"
  ],
  "version": "1.3.3",
  "devDependencies": {
    "@skypack/package-check": "^0.2.2",
    "@types/ejs": "^3.0.6",
    "@types/express": "^4.17.12",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-middleware": "^5.0.0",
    "cypress": "^3.8.3",
    "ejs": "^3.1.6",
    "express": "^4.17.2",
    "html-webpack-plugin": "^5.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "start-server-and-test": "^1.12.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5",
    "webpack": "^5.41.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-middleware": "^5.0.0",
    "webpack-dev-server": "^4.7.1"
  },
  "scripts": {
    "clean": "rm -f ./lib/*; rm -f ./esm/*",
    "compile": "tsc --build ",
    "compile:esm": "tsc --module ES2015 --outDir ./esm --target es2017",
    "build": "npm run clean; npm run compile; npm run compile:esm",
    "example-server": "ts-node ./examples/server.ts --project ./examples/tsconfig.json",
    "cypress": "cypress run",
    "ci": "start-server-and-test example-server 4000 cypress",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "fast-deep-equal": "^3.1.3",
    "prop-types": "^15.8.0"
  },
  "files": [
    "lib/*",
    "esm/*"
  ],
  "main": "lib/index.js",
  "module": "./esm/index.js",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./lib/index.js",
      "default": "./esm/index.js"
    },
    "./esm/": "./esm/",
    "./lib/": "./lib/"
  },
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/dixahq/elevio-client-packages"
  }
}
